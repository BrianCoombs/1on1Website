{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "acuityscheduling": "^0.1.7",
    "express": "^4.16.3",
    "npm": "^5.8.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm server\" \"npm client\"",
